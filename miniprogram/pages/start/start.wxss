/* pages/start/start.wxss */
page {
    height: 100%;
}
.btn {
  width: 180rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: center;
  background-color: rgb(163, 222, 163);
  font-size: 30rpx;
  color: #fff
}

.btn-hover::after{
  background-color:aliceblue;
  color: #000000;
}



.header {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}

.header image {
    width: 200rpx;
    height: 200rpx;
}

.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}

.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}

.bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}
